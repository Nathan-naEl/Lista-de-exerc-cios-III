#include <stdio.h>

int main() {
    int m[3][3];
    int i,j;
    for(i=0;i<3;i++) for(j=0;j<3;j++) {
        printf("m[%d][%d]: ", i, j);
        scanf("%d", &m[i][j]);
    }
    int alvo = 0;
    for(j=0;j<3;j++) alvo += m[0][j];
    int ok = 1;
    for(i=1;i<3;i++) {
        int s = 0;
        for(j=0;j<3;j++) s += m[i][j];
        if (s != alvo) ok = 0;
    }
    for(j=0;j<3;j++) {
        int s = 0;
        for(i=0;i<3;i++) s += m[i][j];
        if (s != alvo) ok = 0;
    }
    int d1 = m[0][0] + m[1][1] + m[2][2];
    int d2 = m[0][2] + m[1][1] + m[2][0];
    if (d1 != alvo || d2 != alvo) ok = 0;
    if (ok) printf("É um quadrado magico\n");
    else printf("Não é um quadrado magico\n");
    return 0;
}
