#include <stdio.h>

int main() {
    int v[10], i, j, cont;
    for(i=0;i<10;i++) {
        printf("v[%d]: ", i);
        scanf("%d", &v[i]);
    }
    int usado[10] = {0};
    for(i=0;i<10;i++) {
        if (usado[i]) continue;
        cont = 1;
        for(j=i+1;j<10;j++) if (v[j]==v[i]) { cont++; usado[j]=1; }
        printf("Valor %d apareceu %d vezes\n", v[i], cont);
    }
    return 0;
}
