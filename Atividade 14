#include <stdio.h>

int main() {
    int m[4][4];
    int i,j;
    for(i=0;i<4;i++) for(j=0;j<4;j++) {
        printf("m[%d][%d]: ", i, j);
        scanf("%d", &m[i][j]);
    }
    int soma = 0;
    for(i=0;i<4;i++) soma += m[i][i];
    printf("Soma da diagonal principal: %d\n", soma);
    return 0;
}
